                                  "type": "string"
                                },
                                "mimeType": {
                                  "type": "string"
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "messageHeader": {
              "properties": {
                "bcc": {
                  "type": "string",
                  "fields": {
                    "unbroken": {
                      "type": "string",
                      "analyzer": "analyzer_keyword_lowercase"
                    }
                  }
                },
                "cc": {
                  "type": "string",
                  "fields": {
                    "unbroken": {
                      "type": "string",
                      "analyzer": "analyzer_keyword_lowercase"
                    }
                  }
                },
                "date": {
                  "type": "string"
                },
                "from": {

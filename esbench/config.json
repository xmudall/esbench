{

    "queries": {

        "match_account": {
            "partial_fields": {
                "partial": {
                    "exclude": [ "mailBody" ]
                }
            },
            "from": 0, 
            "query": {
                "filtered": {
                    "filter": {
                        "term": {
                            "account.unbroken": "jianwche@gmail.com"
                        }
                    }
                }
            }, 
            "size": 50
        }
    },

    "index": {
      "settings": {
        "index": {
          "number_of_shards": "5",
          "number_of_replicas": "1",
          "analysis": {
            "analyzer": {
              "analyzer_keyword_lowercase": {
                "filter": "lowercase",
                "tokenizer": "keyword"
              }
            }
          }
        }
      },
      "mappings": {
        "rush_mail": {
          "properties": {
            "account": {
              "type": "string",
              "fields": {
                "unbroken": {
                  "type": "string",
                  "analyzer": "analyzer_keyword_lowercase"
                }
              }
            },
            "attachments": {
              "properties": {
                "attachmentId": {
                  "type": "string"
                },
                "key": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "url": {
                  "type": "string"
                }
              }
            },
            "bundleId": {
              "type": "long"
            },
            "fields": {
              "type": "string"
            },
            "hbaseRowKey": {
              "type": "string"
            },
            "id": {
              "type": "string"
            },
            "isAnswered": {
              "type": "boolean"
            },
            "isDelete": {
              "type": "boolean"
            },
            "isFlag": {
              "type": "boolean"

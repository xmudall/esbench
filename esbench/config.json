            "preview": {
              "type": "string"
            },
            "query": {
              "properties": {
                "match": {
                  "properties": {
                    "account": {
                      "type": "string"
                    }
                  }
                }
              }
            },
            "rushId": {
              "type": "long"
            },
            "send": {
              "type": "boolean"

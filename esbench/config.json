                  "type": "string"
                },
                "mimeType": {
                  "type": "string"
                },
                "rushMailBodyPartList": {
                  "properties": {
                    "content": {
                      "type": "string"
                    },
                    "mimeType": {
                      "type": "string"
                    },
                    "subRushMailBodyParts": {
                      "properties": {
                        "content": {
                          "type": "string"
                        },
                        "mimeType": {
                          "type": "string"
                        },
                        "subRushMailBodyParts": {
                          "properties": {
                            "content": {
                              "type": "string"
